{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run vercel-build"
      }
    },
    {
      "src": "dist/server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": ".vercel/output/static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "dist/server/index.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}